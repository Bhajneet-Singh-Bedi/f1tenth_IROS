#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'autodrive-f1tenth==0.1.0','console_scripts','autodrive_bridge'
__requires__ = 'autodrive-f1tenth==0.1.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('autodrive-f1tenth==0.1.0', 'console_scripts', 'autodrive_bridge')()
    )
